{
  "expo": {
    "name": "CarCam",
    "slug": "CarCam",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "primaryColor": "#29ABE2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "This app aims to become a cheap alternative to an average driver's dashcam needs.",
    "githubUrl": "https://github.com/taricjain/CarCam"
  }
}